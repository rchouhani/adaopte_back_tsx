{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
         "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "adaopte_back_tsx/wsgi.py"
    }
  ]
}